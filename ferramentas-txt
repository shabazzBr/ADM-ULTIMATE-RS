echo -e "${cor[2]} [4] \033[1;31m>${cor[3]} BAD UDP $_badvpn"
echo -e "${cor[2]} [5] \033[1;31m>${cor[3]} TCP SPEED $_tcpspeed"
echo -e "${cor[2]} [6] \033[1;31m>${cor[3]} FAILBAN $fail_b"
echo -e "${cor[2]} [7] \033[1;31m>${cor[3]} SQUID CACHE $_cachesquid"
echo -e "${cor[2]} [8] \033[1;31m>${cor[3]} COMPARTILHAR ARQUIVO ONLINE"
echo -e "${cor[2]} [9] \033[1;31m>${cor[3]} TESTAR VELOCIDADE"
echo -e "${cor[2]} [10] \033[1;31m>${cor[3]} INFORMAÇÕES VPS"
echo -e "${cor[2]} [11] \033[1;31m>${cor[3]} ADICIONAR/REMOVER HOST-SQUID"
echo -e "${cor[2]} [12] \033[1;31m>${cor[3]} GERADOR DE PAYLOAD"
echo -e "${cor[2]} [13] \033[1;31m>${cor[3]} GERENCIAMENTO DE PORTAS"
echo -e "${cor[2]} [14] \033[1;31m>${cor[3]} ULTRA HOST (ESCANER DE SUBDOMINIOS)"
echo -e "${cor[2]} [15] \033[1;31m>${cor[3]} VNC SERVER"
echo -e "${cor[2]} [16] \033[1;31m>${cor[3]} BRUTE FORCE PAYLOAD"
echo -e "${cor[2]} [17] \033[1;31m>${cor[3]} BLOQUEAR TORRENT"
echo -e "${cor[2]} [18] \033[1;31m>${cor[3]} REINICIAR VPS (REBOOT)"
echo -e "${cor[2]} [0] \033[1;31m>\033[1;37m [ VOLTAR / SAIR ]"
echo -e "${cor[1]} ====================================================== ${cor[0]}"
