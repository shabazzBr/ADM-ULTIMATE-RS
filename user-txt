#!/bin/bash
source cabecalho
echo -e "${cor[1]} =×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=× ${cor[0]}"
echo -e "${cor[2]}${cor[5]} MENU USUARIOS"
echo -e "${cor[1]} =×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=× ${cor[0]}"
echo -e "${cor[2]} [0] >${cor[4]} [ VOLTAR / SAIR ]"
echo -e "${cor[2]} [1] >${cor[3]} CRIAR USUARIO"
echo -e "${cor[2]} [2] >${cor[3]} CRIAR USUARIO"
echo -e "${cor[2]} [3] >${cor[3]} EDITAR USUARIO"
echo -e "${cor[2]} [4] >${cor[3]} DETALHES DOS USUARIOS"
echo -e "${cor[2]} [5] >${cor[3]} MONITORAR USUARIOS ONLINE"
echo -e "${cor[2]} [6] >${cor[3]} EXCLUIR USUARIOS VENCIDOS"
echo -e "${cor[2]} [7] >${cor[3]} BANNER COLORIDO"
echo -e "${cor[2]} [8] >${cor[3]} MONITOR DE CONSUMO"
echo -e "${cor[2]} [9] >${cor[3]} LIMITER CONEXÃO SIMULTÂNEA $limiter_on"
echo -e "${cor[1]} =×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×=×  ${cor[0]}"
